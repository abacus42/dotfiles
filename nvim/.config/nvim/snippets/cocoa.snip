snippet define
alias Define
    define ${1}(${2})
        ${0}
    enddefine;

snippet if
alias If
    if ${1} then
        ${0}
    endif;

snippet for
alias For
    for ${1} do
        ${0}
    endfor;

snippet foreach
alias Foreach
    foreach ${1} do
        ${0}
    endforeach;

snippet while
alias While
    while ${1} do
        ${0}
    endwhile;

snippet using
alias Using
    using ${1} do
        ${0}
    endusing;

snippet package
alias Package
    package \$${0}
    endpackage;
